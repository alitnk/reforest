{
  "name": "reforest",
  "version": "0.9.1",
  "description": "Collect ordered React render data seamlessly across the server and client.",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepublishOnly": "pnpm test && pnpm build",
    "build": "tsup src/index.ts --dts --format esm,cjs --legacy-output",
    "dev": "pnpm build --watch --sourcemap",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/reforest"
  },
  "keywords": [
    "react",
    "index",
    "accessibility",
    "select",
    "utility",
    "list",
    "tree",
    "node",
    "store"
  ],
  "author": "Travis Arnold <travis@souporserious.com> (http://souporserious.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/souporserious/reforest/issues"
  },
  "homepage": "https://github.com/souporserious/reforest",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "jotai": "^1.7.8",
    "performant-array-to-tree": "^1.11.0",
    "suspend-react": "^0.0.8"
  },
  "devDependencies": {
    "@jsxui/layout": "^0.1.3",
    "react": "*",
    "react-dom": "*",
    "tsup": "^6.2.2",
    "typescript": "*"
  }
}
